<%
    const cssExtNameMap = { sass: 'scss', stylus: 'styl', less: 'less', tailwindcss: 'css' };
%>
<%
    const cssExtName = cssExtNameMap[cssProcessor];
%>
import './index.<%= cssExtName %>';
<% if (cssProcessor === 'tailwindcss') { %>import './tailwind.css';<% } %>

import App from './App.svelte';
const app = new App({
    target: document.getElementById('app'),
});

export default app;
